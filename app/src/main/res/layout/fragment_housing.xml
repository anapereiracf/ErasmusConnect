<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".ui.Housing">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="800dp"
        android:layout_height="1051dp"
        android:src="@drawable/backbround"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.359"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.34" />

    <TextView
        android:id="@+id/housingtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="Housing Resources"
        android:textAppearance="@style/custom_headline4"
        android:textColor="@color/primary_white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/erasmusplaycard"
        android:layout_width="110dp"
        android:layout_height="125dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        android:foregroundGravity="center|right"
        app:cardBackgroundColor="#86FFFFFF"
        app:cardCornerRadius="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/housingtitle"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="16dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/erasmusplaylogo"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/erasmus_play_no_bg" />

            <TextView
                android:id="@+id/erasmusplaytext"
                android:text="Erasmus Play"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/archivo_semibold"
                android:textAppearance="@style/custom_body2"
                android:textColor="@color/primary_blue" />
        </LinearLayout>


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cafoscaricard"
        android:layout_width="110dp"
        android:layout_height="125dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        android:foregroundGravity="center|right"
        app:cardBackgroundColor="#86FFFFFF"
        app:cardCornerRadius="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/erasmusplaycard"
        app:layout_constraintTop_toBottomOf="@+id/housingtitle"
        app:layout_constraintVertical_bias="0.0" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="16dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/cflogo"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/cf_logo"
                app:tint="#A11F1F" />

            <TextView
                android:id="@+id/cftext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/archivo_semibold"
                android:textAppearance="@style/custom_body2"
                android:textColor="@color/primary_blue"
                android:text="Ca' Foscari Housing" />

        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <View
        android:id="@+id/div"
        android:layout_width="2dp"
        android:layout_height="450dp"
        android:layout_marginTop="80dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.458"
        app:layout_constraintLeft_toRightOf="@+id/erasmusplaycard"
        app:layout_constraintRight_toLeftOf="@+id/cafoscaricard"
        app:layout_constraintTop_toBottomOf="@+id/housingtitle"
        app:layout_constraintVertical_bias="0.200" />

    <androidx.cardview.widget.CardView
        android:id="@+id/erasmusucard"
        android:layout_width="110dp"
        android:layout_height="125dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        android:foregroundGravity="center|right"
        app:cardBackgroundColor="#86FFFFFF"
        app:cardCornerRadius="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/erasmusplaycard"
        app:layout_constraintVertical_bias="0.0" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="16dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/erasmusulogo"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/erasmusu" />

            <TextView
                android:id="@+id/erasmusutext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/archivo_semibold"
                android:textAppearance="@style/custom_body2"
                android:textColor="@color/primary_blue"
                android:text="Erasmusu" />
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/rentolacard"
        android:layout_width="110dp"
        android:layout_height="125dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        android:foregroundGravity="center|right"
        app:cardBackgroundColor="#86FFFFFF"
        app:cardCornerRadius="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.647"
        app:layout_constraintStart_toStartOf="@+id/erasmusucard"
        app:layout_constraintTop_toBottomOf="@+id/cafoscaricard"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="16dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/rentolalogo"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/rentola" />

            <TextView
                android:id="@+id/rentolatext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/archivo_semibold"
                android:text="Rentola"
                android:textAppearance="@style/custom_body2"
                android:textColor="@color/primary_blue" />
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <!--XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX-->
    <androidx.cardview.widget.CardView
        android:id="@+id/housinganycard"
        android:layout_width="110dp"
        android:layout_height="125dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        android:foregroundGravity="center|right"
        app:cardBackgroundColor="#86FFFFFF"
        app:cardCornerRadius="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/erasmusucard"
        app:layout_constraintVertical_bias="0.0" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="16dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/housinganylogo"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/housing_anywhere_logo" />

            <TextView
                android:id="@+id/housinganytext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/archivo_semibold"
                android:textAppearance="@style/custom_body2"
                android:textColor="@color/primary_blue"
                android:text="Housing Anywhere" />
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/uniplacescard"
        android:layout_width="110dp"
        android:layout_height="125dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        android:foregroundGravity="center|right"
        app:cardBackgroundColor="#86FFFFFF"
        app:cardCornerRadius="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.647"
        app:layout_constraintStart_toStartOf="@+id/housinganycard"
        app:layout_constraintTop_toBottomOf="@+id/rentolacard"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="16dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/uniplaceslogo"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/uniplaces_logo" />

            <TextView
                android:id="@+id/uniplacestext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/archivo_semibold"
                android:text="Uniplaces"
                android:textAppearance="@style/custom_body2"
                android:textColor="@color/primary_blue" />
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <ImageButton
        android:id="@+id/homebutton2"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:minHeight="48dp"
        android:src="?attr/actionModeCloseDrawable"
        app:circularflow_defaultRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/housinganycard"
        app:layout_constraintTop_toBottomOf="@id/housinganycard"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>